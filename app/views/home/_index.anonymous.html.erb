  <div class="span5">
    <div class="hero-unit slim linen word-breaker">
      <h1>Save It</h1>
      <p>Simple, secure bookmarks</p>
      <hr/>
      <ul class="nav nav-tabs">
        <li class="active">
          <a href="#signin-tab" data-toggle="tab"><h2>Sign in</h2></a>
        </li>
        <li><a href="#signup-tab" data-toggle="tab"><h2>Sign up</h2></a></li>
      </ul>
      <div class="tab-content">
        <div class="tab-pane active" id="signin-tab">
          <div id="login"></div>
        </div>
        <div class="tab-pane" id="signup-tab">
          <div id="signup"></div>
        </div>
      </div>
      <div class="control-group">
        <h3>Or sign in via social account</h3>
      </div>
      <div class="control-group">
        <%= link_to user_omniauth_authorize_path(:facebook) do %><%= image_tag 'icon-fb-50.png'%><% end %>
        <%= link_to user_omniauth_authorize_path(:twitter) do %><%= image_tag 'icon-twitter-50.png'%><% end %>
      </div>
    </div>
  </div>



