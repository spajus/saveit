<td>
<a class="link" href="<%= @bookmark.get 'url' %>"><%= @bookmark.get('title') or @bookmark.get('url') %></a>
</td>
<% if user_settings.getUseTags(): %>
<td class="tags">
  <a href="#" rel="tooltip" title="Drag and drop me on a tag!"><i data-bookmark-id="<%= @bookmark.get 'id' %>" class="icon-tag icon-large"></i></a>
</td>
<% end %>
<td>
  <a class="close">x</a>
</td>

