<td>

  <div class="row-fluid">
      <a class="link" href="<%= @bookmark.get 'url' %>"><%= @bookmark.get('title') or @bookmark.get('url') %></a>
      <a class="close">x</a>
  </div>
  <div class="row-fluid">
    <% if user_settings.getUseTags(): %>
      <div class="tags">
      <i title="Drop me on a tag!" data-bookmark-id="<%= @bookmark.get 'id' %>" class="icon-tag icon-large"></i>
      </div>
    <% end %>
  </div>

</td>

